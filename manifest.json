{
  "manifest_version": 2,
  "name": "Pasted Chrome Extension",
  "short_name": "PCE",
  "description": "Paste content from within a browser window with the power of right clicks",
  "version": "0.0.3",
  "minimum_chrome_version": "38",
  "permissions": [
    "contextMenus",
    "activeTab",
    "https://pasted.tech/*"
  ],
  "icons": {
    "16": "assets/pencil.png"
  },
  "background": {
    "page": "background.html"
  }
}
